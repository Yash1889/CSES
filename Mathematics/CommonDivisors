#include <bits/stdc++.h>
using namespace std;
 
int main(){
    int n;
    cin>>n;
    vector<int> v(n);
    for(int i=0;i<n;i++){
        cin>>v[i];
    }
 
    int maxi = *max_element(v.begin(),v.end());
 
    vector<int> freq(maxi+1,0);
    for(auto p:v)freq[p]++;
    for(int i=maxi;i>=1;i--){
        int count = 0;
        for(int p=i;p<=maxi;p+=i){
            count+=freq[p];
        }        
        if(count>=2){
            cout<<i;
            break;
        }
    }
}
