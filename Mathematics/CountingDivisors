#include <bits/stdc++.h>
using namespace std;
 
 
 
 
int main(){
 
    const int N = 1e6;
 
    vector<int> pre(N+1,0);
 
    for(int i=1;i<=N;i++){
        for(int j=i;j<=N;j+=i){
            pre[j]++;
        }
    }
 
    int n;
    cin>>n;
    while(n--){
        int k;
        cin>>k;
        cout<<pre[k]<<'\n';
    }
}   
